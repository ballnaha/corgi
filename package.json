{
  "name": "corgi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && npm run sitemap:generate",
    "build:only": "next build",
    "sitemap:generate": "echo 'Sitemap will be generated automatically by Next.js during build'",
    "start": "next start",
    "lint": "next lint",
    "db:setup": "node scripts/setup-database.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:categories": "tsx prisma/seeds/category-data.ts",
    "db:seed:products": "tsx prisma/seeds/product-data.ts",
    "db:seed:payment-methods": "node scripts/seed-payment-methods.js",
    "db:backfill:categories": "node scripts/backfill-category-ids.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/prompt": "^5.2.6",
    "@line/liff": "^2.27.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.0",
    "@prisma/client": "^6.13.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "dayjs": "^1.11.13",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-image-crop": "^10.1.8",
    "react-markdown": "^10.1.0",
    "sharp": "^0.34.3",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@mui/material-nextjs": "^6.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5"
